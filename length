def bubbleSort(list):
    for i in range(len(list)):
        if i+1 < len(list) and list[i+1] > list[i]:
            list[i], list[i+1] = list[i+1], list[i]
            
            #while for i in range(len(list)):
                  
        if i+1 < len(list) and list[i+1] > list[i]:
            list[i], list[i+1] = list[i+1], list[i]
DeseanList = []
for i in range(20):
    DeseanList.append(note(random.randrange(400, 2000), 300))

for i in range(20):
    DeseanList[i].PlaySound()

