import winsound
import random


class note:
    
    def __init__(self, p, l):
        self.pitch = p
        self.length = l
        
    def PlaySound(self):
        winsound.Beep(self.pitch, self.length)
        
    def PrintInfo(self):
        print("I'm a note of pitch", self.pitch, "and length", self.length)
   
